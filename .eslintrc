{
  "env": {
    "browser": true
  },
  "extends": "airbnb",
  "rules": {
    "linebreak-style": 0,
    "react/jsx-filename-extension": 0,
    "jsx-a11y/label-has-for": [ 2, {
      "components": [ "Label" ],
      "required": {
        "some": [ "nesting", "id" ]
      },
      "allowChildren": false
    }],
    "import/first": 0
  }
}
